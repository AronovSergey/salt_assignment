{
  "editor.tabSize": 2,
  "editor.fontSize": 14,
  "editor.mouseWheelZoom": true,
  "eslint.validate": [
    "html",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnSave": true,
  "window.zoomLevel": 1,

  "eslint.workingDirectories": ["./"],
  "prettier.configPath": "./.prettierrc.js"
}
